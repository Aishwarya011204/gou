{
  "name": "用户",
  "table": {
    "name": "user",
    "comment": "用户表",
    "engine": "InnoDB"
  },
  "columns": [
    { "name": "id", "type": "ID" },
    {
      "name": "manu_id",
      "type": "bigInteger",
      "length": 50,
      "comment": "所属厂商",
      "nullable": true,
      "index": true
    },
    {
      "name": "type",
      "type": "enum",
      "option": ["admin", "staff"],
      "comment": "账号类型 admin 管理员, staff 员工",
      "default": "staff",
      "index": true
    },
    {
      "name": "mobile",
      "type": "string",
      "length": 50,
      "comment": "手机号",
      "index": true,
      "crypt": "AES"
    },
    {
      "name": "password",
      "type": "string",
      "length": 256,
      "comment": "登录密码",
      "crypt": "PASSWORD",
      "index": true
    },
    {
      "name": "name",
      "type": "string",
      "length": 80,
      "comment": "姓名",
      "index": true
    },
    {
      "name": "idcard",
      "type": "string",
      "length": 256,
      "comment": "身份证号码",
      "crypt": "AES",
      "nullable": true,
      "index": true
    },
    {
      "name": "balance",
      "type": "integer",
      "length": 20,
      "comment": "账户余额",
      "default": 0,
      "index": true
    },
    {
      "name": "key",
      "type": "string",
      "length": 256,
      "comment": "API Key",
      "crypt": "AES",
      "nullable": true,
      "unique": true
    },
    {
      "name": "secret",
      "type": "string",
      "length": 256,
      "nullable": true,
      "crypt": "AES",
      "comment": "API 密钥",
      "index": true
    },
    {
      "name": "resume",
      "type": "text",
      "comment": "简历",
      "nullable": true
    },
    {
      "name": "extra",
      "type": "json",
      "comment": "扩展信息",
      "nullable": true
    },
    {
      "comment": "用户状态 enabled 有效, disabled 无效",
      "name": "status",
      "type": "enum",
      "default": "enabled",
      "option": ["enabled", "disabled"],
      "index": true
    }
  ],
  "relationships": [],
