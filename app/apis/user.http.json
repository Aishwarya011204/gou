{
  "name": "用户接口",
  "version": "1.0.0",
  "description": "用户API",
  "group": "user",
  "guard": "bearer-jwt",
  "paths": [
    {
      "path": "/login",
      "method": "POST",
      "guard": "-",
      "process": "plugins.user.Login",
      "in": ["$payload.mobile", "$payload.password", "$payload.captcha"],
      "out": {
        "status": 200,
        "type": "application/json"
      }
    },
    {
      "path": "/info/session/in",
      "method": "GET",
      "process": "models.user.Find",
      "in": ["$session.id"],
      "out": {
        "status": 200,
        "type": "application/json"
      }
    },
    {
      "path": "/info/session/flow",
      "method": "GET",
      "process": "flows.user.info",
      "in": [],
      "out": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json",
          "User-Agent": "?:user.id"
        }
      }
    },
    {
      "path": "/info/:id",
      "method": "GET",
      "process": "models.user.Find",
      "in": ["$param.id", ":params"],
      "out": {
        "status": 200,
        "type": "application/json"
      }
    }
  ]
}
