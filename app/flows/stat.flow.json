{
  "label": "统计查询",
  "version": "1.0.0",
  "description": "Gou QueryDSL 统计查询测试",
  "nodes": [
    {
      "name": "manus",
      "engine": "test-db",
      "query": {
        "select": ["id", "short_name", "type"],
        "from": "$manu",
        "wheres": [
          { ":status": "状态", "=": "enabled" },
          { ":created_at": "创建时间", ">": "?:$in.0" },
          { ":created_at": "创建时间", "<": "?:$in.1" }
        ],
        "limit": "?:$in.3"
      },
      "outs": ["{{$out}}"]
    },
    {
      "name": "users",
      "engine": "test-db",
      "query": {
        "select": ["id", "name", "extra$", "resume"],
        "from": "$user",
        "orders": "created_at desc",
        "wheres": [
          { ":status": "状态", "=": "enabled" },
          { ":created_at": "创建时间", ">": "?:$in.0" },
          { ":created_at": "创建时间", "<": "?:$in.1" }
        ],
        "page": "?:$in.2",
        "pagesize": "?:$in.3"
      },
      "outs": ["{{$out}}"]
    },
    {
      "name": "address",
      "engine": "test-db",
      "query": {
        "select": ["province", "city", "id"],
        "from": "$address",
        "orders": "created_at desc",
        "wheres": [
          { ":status": "状态", "=": "enabled" },
          { ":created_at": "创建时间", ">": "?:$in.0" },
          { ":created_at": "创建时间", "<": "?:$in.1" }
        ],
        "first": true
      }
    }
  ],
  "output": {
    "params": "{{$in}}",
    "data": {
      "manus": "{{$res.manus.0}}",
      "users": "{{$res.users.0}}",
      "address": "{{$res.address}}"
    }
  }
}
